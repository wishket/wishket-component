@font-face {
    font-family: 'NotoSansKR';
    font-weight: 700;
    src:
        url('/static/renewal/fonts/NotoSansKR-Bold.woff2') format('woff2'),
        url('/static/renewal/fonts/NotoSansKR-Bold.woff') format('woff'),
        url('/static/renewal/fonts/NotoSansKR-Bold.otf') format('opentype');
}

